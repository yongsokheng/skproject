      <!-- ads_homepage -->
      <ins class="adsbygoogle"
           style="display:block"
           data-ad-client="ca-pub-1391132134348179"
           data-ad-slot="2008653046"
           data-ad-format="auto"></ins>
      <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
      </script>
    </div>
